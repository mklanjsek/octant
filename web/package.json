    "@angular/language-service": "^10.1.1",
    "@babel/core": "^7.11.6",
    "@compodoc/compodoc": "^1.1.11",
    "@mdx-js/loader": "^1.6.16",
    "@storybook/addon-actions": "^v6.0.21",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-docs": "^v6.0.21",
    "@storybook/addon-knobs": "^v6.0.21",
    "@storybook/addon-links": "^v6.0.21",
    "@storybook/addons": "^v6.0.21",
    "@storybook/angular": "^v6.0.21",
    "@storybook/theming": "^v6.0.21",
    "@types/d3": "~5.7.2",
    "@types/graphlib-dot": "^0.6.1",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.161",
    "@types/node": "^12.12.56",
    "@types/ws": "^7.2.6",
    "babel-loader": "^8.1.0",
