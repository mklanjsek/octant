    "karma": "^5.2.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "karma-spec-reporter": "0.0.32",
    "prettier": "^2.1.1",
    "prettier-tslint": "^0.4.2",
    "protractor": "~7.0.0",
    "react": "^16.13.1",
    "react-docgen": "^5.3.0",
    "react-is": "^16.13.1",
    "ts-loader": "^7.0.5",
    "ts-mockito": "^2.6.1",
    "ts-node": "8.8.1",
    "ts-protoc-gen": "^0.12.0",
    "tslint": "~6.1.0",
